###############################################################################
# Generated by Chef for <%= node[:hostname] %>
###############################################################################

/var/log/logstash/*.log {
    size=20M
    missingok
    rotate 5
    compress
    delaycompress
    notifempty
    copytruncate
    postrotate
        rm -f /var/log/logstash/*.gz > /dev/null
    endscript
}
